#include "main.h"
#include <stdio.h>

/**
  * crackme_password  - Create a file that contains the password for the crackme2 executable.
  * 
  * 
  *
  * Return: Nothing.
  */
  
int main() {
unsigned char digest[MD5_DIGEST_LENGTH];
char string[] = "password";

MD5((unsigned char*)&string, strlen(string), (unsigned char*)&digest);

int i;
for(i = 0; i < MD5_DIGEST_LENGTH; i++)
printf("%02x", digest[i]);

return 0;
}